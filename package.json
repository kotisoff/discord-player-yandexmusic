{
  "name": "discord-player-yandexmusic",
  "version": "0.1.5",
  "description": "A custom extractor made for discord-player that enables you to extract from Yandex Music.",
  "main": "dist/YandexMusicExtractor.js",
  "author": "kotisoff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kotisoff/discord-player-yandexmusic.git"
  },
  "dependencies": {
    "discord-player": "^6.6.10",
    "tslint-config-prettier": "^1.18.0",
    "ym-api-meowed": "^1.3.4"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "npm run lint && tsc && npm run format",
    "format": "prettier --write \"dist/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "npm:publish": "yarn build && npm publish --access public"
  }
